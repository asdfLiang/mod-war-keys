<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="449.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.front.controller.ModKeyController">

    <Button alignment="TOP_LEFT" layoutX="449.0" layoutY="35.0" onAction="#selectFile" text="选择配置文件" />

    <TextField fx:id="configPathInput" alignment="TOP_LEFT" disable="true" layoutX="63.0" layoutY="35.0" prefHeight="23.0" prefWidth="377.0" promptText="配置文件路径" />

    <TableView fx:id="tableView" layoutX="63.0" layoutY="69.0" prefHeight="307.0" prefWidth="474.0">
        <columns>
            <TableColumn fx:id="cmdType" prefWidth="125.0" sortable="false" text="指令类型" />
            <TableColumn fx:id="cmd" prefWidth="109.0" sortable="false" text="指令" />
            <TableColumn fx:id="cmdName" prefWidth="163.0" sortable="false" text="指令名称" />
            <TableColumn fx:id="hotKey" prefWidth="62.0" sortable="false" text="快捷键" />
        </columns>
    </TableView>

    <Button alignment="TOP_LEFT" layoutX="441.0" layoutY="398.0" text="确定" />

    <Button alignment="TOP_LEFT" layoutX="497.0" layoutY="398.0" onAction="#quit" text="退出" />

</AnchorPane>
